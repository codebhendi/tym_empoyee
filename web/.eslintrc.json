{
  "parser": "babel-eslint",
  "plugins": [
    "jsx-a11y", "import"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb",
  "rules": {
    "jsx-a11y/label-has-for": [2, {
      "required": {
        "every": ["id"]
      },
      "allowChildren": false
    }],
    "jsx-a11y/label-has-associated-control": 0,
    "no-console": 0,
    "defaultAssignment": 0,
    "no-nested-ternary": 0,
    "import/no-cycle": 0,
    "react/prop-types": 2,
    "react/jsx-props-no-spreading": 0
  }
}
